{
  "name": "sasi-home",
  "version": "1.0.0",
  "description": "SASI@home - Search for Artificial Super Intelligence with Synaptic-mesh integration",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:performance": "jest --testPathPatterns=tests/performance --detectLeaks",
    "test:persistence": "jest --testPathPatterns=tests/persistence",
    "test:coordination": "jest --testPathPatterns=tests/coordination",
    "test:tdd": "jest --testPathPatterns=tests/tdd",
    "test:e2e": "node tests/run-e2e.js",
    "test:e2e-direct": "jest tests/e2e/",
    "test:all": "node tests/test-runner.js",
    "test:phase2a": "node tests/test-runner-phase2a.js",
    "test:phase2a-fast": "jest --testPathPatterns='tests/(integration|persistence|coordination|performance)'",
    "test:benchmarks": "jest --testPathPatterns=tests/performance/phase2a-performance-benchmarks --runInBand",
    "test:benchmarks-automated": "jest --testPathPatterns=tests/performance/phase2a-automated-benchmarks --runInBand",
    "test:transition": "jest --testPathPatterns=tests/integration/mock-neural-transition-validation",
    "test:transition-comprehensive": "jest --testPathPatterns='tests/integration/(phase2a-comprehensive-integration|mock-neural-transition-validation)'",
    "test:e2e-workflows": "jest --testPathPatterns=tests/e2e/phase2a-end-to-end-workflows --runInBand --testTimeout=300000",
    "test:coverage": "jest --coverage",
    "test:coverage-phase2a": "jest --coverage --testPathPatterns='tests/(integration|persistence|coordination|performance)' --coverageReporters=text,lcov,html",
    "test:watch": "jest --watch",
    "test:watch-phase2a": "jest --watch --testPathPatterns='tests/(integration|persistence|coordination)'",
    "test:ci": "node scripts/ci-test-runner.js",
    "test:ci-phase2a": "node tests/test-runner-phase2a.js --ci",
    "test:stress": "jest --testPathPatterns=tests/performance --runInBand --detectLeaks --forceExit",
    "test:memory-leaks": "jest --testPathPatterns='tests/(integration|performance)' --detectLeaks --runInBand",
    "test:regression": "jest --testPathPatterns=tests/performance/phase2a-automated-benchmarks --testNamePattern='regression' --runInBand",
    "test:report": "node scripts/test-reporter.js",
    "test:full-ci": "npm run test:ci && npm run test:report",
    "test:regression-detection": "jest --testPathPatterns=tests/performance/regression-detector",
    "test:automation": "npm run test:full-ci",
    "test:wasm": "jest --testPathPattern=tests/wasm --testTimeout=180000",
    "test:synaptic": "jest --testPathPattern=synaptic-mesh/tests",
    "ci:lint": "eslint src --ext ts,tsx --format json --output-file eslint-results.json",
    "ci:audit": "npm audit --audit-level=high --json > security-audit.json || true",
    "ci:build-validate": "npm run build && node scripts/validate-build.js",
    "ci:performance-baseline": "npm run test:regression-detection -- --updateSnapshot"
  },
  "dependencies": {
    "@react-three/drei": "^9.92.0",
    "@react-three/fiber": "^8.15.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/three": "^0.160.0",
    "better-sqlite3": "^12.2.0",
    "claude-flow": "^2.0.0-alpha.43",
    "jest-environment-jsdom": "^30.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "ruv-swarm": "^1.0.14",
    "three": "^0.160.0",
    "ts-jest": "^29.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^30.0.4",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-node": "^30.0.4",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "puppeteer": "^24.11.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "keywords": [
    "sasi",
    "artificial-intelligence",
    "swarm",
    "visualization",
    "webgl",
    "three.js",
    "synaptic-mesh",
    "neural-networks",
    "claude-flow"
  ],
  "author": "SASI@home Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cgbarlow/sasi.git"
  }
}
